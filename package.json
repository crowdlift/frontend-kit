{
  "name": "frontend-kit",
  "version": "0.1.2",
  "description": "Crowdlift website kit.",
  "private": true,
  "main": "./index.js",
  "bin": {
    "cross-env": "./node_modules/.bin/cross-env",
    "eslint": "./node_modules/.bin/eslint",
    "http-server": "./node_modules/.bin/http-server",
    "npm-run-all": "./node_modules/.bin/npm-run-all",
    "rimraf": "./node_modules/.bin/rimraf",
    "webpack-dev-server": "./node_modules/.bin/webpack-dev-server"
  },
  "repository": "crowdlift/frontend-kit",
  "homepage": "https://github.com/simple10/crowdwebkit#readme",
  "scripts": {
    "build": "npm-run-all -p build:prod",
    "build:dev": "cross-env NODE_ENV=development BUILD_DIR=./dist/dev webpack --display-chunks --progress",
    "build:prod": "cross-env NODE_ENV=production webpack --display-chunks --progress",
    "clean": "rimraf dist",
    "deploy": "./bin/deploy-gh.sh",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "lint": "eslint src *.js",
    "prebuild": "cross-env RELEASE=1 npm-run-all -p clean test",
    "serve": "./node_modules/.bin/http-server -p 2000 dist/",
    "start": "npm run dev",
    "test": "echo TESTS PLACEHOLDER"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "babel-register": "^6.23.0",
    "bundle-loader": "^0.5.5",
    "compression-webpack-plugin": "^0.3.2",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.2",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-prefer-object-spread": "^1.1.0",
    "eslint-plugin-react": "^6.10.0",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "http-server": "^0.9.0",
    "imports-loader": "^0.7.1",
    "lodash": "^4.17.4",
    "npm-run-all": "^4.0.2",
    "postcss-cssnext": "^2.9.0",
    "postcss-flexbugs-fixes": "^2.1.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.2",
    "pug": "^2.0.0-beta11",
